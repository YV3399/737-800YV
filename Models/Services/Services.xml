<PropertyList>

<!-- Baggage (Front)-->

  <model>
    <name>Baggage</name>
    <path>Aircraft/737-800/Models/Services/Baggage/baggage.xml</path>
    <condition>
      <and>
        <property>controls/switches/baggage1</property>
        <less-than>
          <property>velocities/airspeed-kt</property>
          <value>15</value>
        </less-than>
      </and>
    </condition>
    <offsets>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </offsets>
  </model>

<!-- Baggage (Front)-->

  <model>
    <name>baggage</name>
    <path>Aircraft/737-800/Models/Services/Baggage/baggage2.xml</path>
    <condition>
      <and>
        <property>controls/switches/baggage2</property>
        <less-than>
          <property>velocities/airspeed-kt</property>
          <value>15</value>
        </less-than>
      </and>
    </condition>
    <offsets>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </offsets>
  </model>

<!-- Fuel truck -->

  <model>
    <name>Fuel truck</name>
    <path>Aircraft/737-800/Models/Services/Fuel/fuel.xml</path>
    <condition>
      <and>
        <property>controls/switches/Fuel</property>
        <less-than>
          <property>velocities/airspeed-kt</property>
          <value>15</value>
        </less-than>
      </and>
    </condition>
    <offsets>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </offsets>
  </model>


<!-- Catering Vehicle Front -->

  <model>
    <name>Cateringback Vehicle</name>
    <path>Aircraft/737-800/Models/Services/Catering/catering.xml</path>
    <condition>
      <equals>
        <property>controls/switches/catering1</property>
        <value type="bool">true</value>
      </equals>
      <less-than>
        <property>velocities/airspeed-kt</property>
        <value>15</value>
      </less-than>
    </condition>
    <offsets>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </offsets>
  </model>

<!-- Catering Vehicle Rear -->

  <model>
    <name>Cateringback Vehicle</name>
    <path>Aircraft/737-800/Models/Services/Catering/catering2.xml</path>
    <condition>
      <equals>
        <property>controls/switches/catering2</property>
        <value type="bool">true</value>
      </equals>
      <less-than>
        <property>velocities/airspeed-kt</property>
        <value>15</value>
      </less-than>
    </condition>
    <offsets>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </offsets>
  </model>
  
  <!-- Bus -->

  <model>
    <name>Bus</name>
    <path>Aircraft/737-800/Models/Services/Bus/bus.xml</path>
    <condition>
      <and>
        <property>controls/switches/bus</property>
        <less-than>
          <property>velocities/airspeed-kt</property>
          <value>15</value>
        </less-than>
      </and>
    </condition>
    <offsets>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </offsets>
  </model>

</PropertyList>
