<?xml version="1.0"?>
<PropertyList>
  <path>737-800.ac</path>
  <animation n="0">
    <texture>737-800.png</texture>
  </animation>
  <model n="100">
    <name>Cockpit</name>
    <offsets>
      <x-m>-18.32</x-m>
      <y-m>0.0</y-m>
      <z-m>0.0</z-m>
      <pitch-deg>0</pitch-deg>
    </offsets>
