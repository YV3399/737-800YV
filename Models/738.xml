<!-- Fuel Truck -->

<model>
  <name>fuel-truck</name>
  <path>Aircraft/737-800/Models/Services/Fuel-Truck/Fuel_Truck_Short_VolvoFM.xml</path>
  <offsets>
    <x-m>1.8</x-m>
    <y-m>12</y-m>
    <z-m>1.81</z-m>
  </offsets>
  <condition>
    <and>
      <less-than> 
        <property>/velocities/groundspeed-kt</property>
        <value>1</value>
      </less-than>
      <equals>
       <property>/services/fuel-truck/enable</property>
       <value>1</value>
     </equals>
   </and>
 </condition>
</model>

