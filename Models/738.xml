      <x-m>-16.5</x-m>
      <y-m>0</y-m>
      <z-m>-2.75</z-m>
    </offsets>
  </model>
  
  <nasal n="0">
    <load>
      var liveries = getprop("/sim/liveries-path");
      var livery_update = aircraft.livery_update.new(liveries,10);
    </load>
    <unload>
      livery_update.stop();
    </unload>
  </nasal>
  
