<PropertyList>

<path>boeing.ac</path>

 	<!-- Power on/off -->
	<animation>
		<name>Power on/off</name>
		<type>select</type>
		<object-name>Off</object-name>
		<condition>
			<and>
			<less-than>
				<property>systems/electrical/bus/dcL</property>
				<value>15</value>
			</less-than>
			<less-than>
				<property>systems/electrical/bus/dcR</property>
				<value>15</value>
			</less-than>
			</and>
		</condition>
	</animation>

<animation>
	<type>material</type>
	<texture alias="../../texture"/>
</animation>

