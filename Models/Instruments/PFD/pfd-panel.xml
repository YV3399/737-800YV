<?xml version="1.0"?>
<PropertyList>
  
<name>PFD Panel</name>
<background>Models/Instruments/PFD/transparent.png</background>
<w>320</w>
<h>310</h>
  
<instruments>
     
    
    <instrument include="hdg.xml">
    <condition>
        <greater-than>
            <property>systems/electrical/outputs/efis</property>
            <value>15</value>
        </greater-than>
    </condition>
        <name>HDG</name>
        <x>112.50</x>
        <y>22.0</y>
        <w>134</w>
        <h>40</h>
    </instrument>

    <instrument include="ai.xml">
    <condition>
        <greater-than>
           <property>systems/electrical/outputs/efis</property>
            <value>15</value>
        </greater-than>
    </condition>
        <name>AI</name>
        <x>113</x>
        <y>133.0</y>
        <w>138</w>
        <h>132</h>
    </instrument> 

     <instrument include="raising-rwy.xml">
    <condition>
        <and>
            <greater-than>
                <property>systems/electrical/outputs/efis</property>
                <value>15</value>
            </greater-than>
            
             <greater-than>
           <property>surface-positions/flap-pos-norm</property>
            <value>0.75</value>
           </greater-than>
        </and>
    </condition>
        <name>rising-rwy</name>
        <x>113.0</x>
        <y>90.0</y>
        <w>120</w>
        <h>70</h>
    </instrument>  

    <instrument include="speed.xml">
    <condition>
        <greater-than>
            <property>systems/electrical/outputs/efis</property>
            <value>15</value>
        </greater-than>
    </condition>
        <name>Spd</name>
        <x>24</x>
        <y>134</y>
        <w>34</w>
        <h>178</h>
    </instrument>

     <instrument include="vsi.xml">
    <condition>
        <greater-than>
            <property>systems/electrical/outputs/efis</property>
            <value>15</value>
        </greater-than>
    </condition>
        <name>vsi</name>
        <x>240</x>
        <y>134</y>
        <w>26</w>
        <h>120</h>
    </instrument>

    <instrument include="alttape.xml">
    <condition>
        <greater-than>
            <property>systems/electrical/outputs/efis</property>
            <value>15</value>
        </greater-than>
    </condition>
        <name>Alttape</name>
        <x>205</x>
        <y>133</y>
        <w>33</w>
        <h>176</h>
    </instrument>

    <instrument include="fd.xml">
    <condition>
        <and>
            <greater-than>
                <property>systems/electrical/outputs/efis</property>
                <value>15</value>
            </greater-than>
            <property>instrumentation/flightdirector/AP-mode</property>
        </and>
    </condition>
        <name>fdbars</name>
        <x>112.75</x>
        <y>133.2</y>
        <w>128</w>
        <h>110</h>
    </instrument>
     
    <instrument include="mask.xml">
    <condition>
        <greater-than>
            <property>systems/electrical/outputs/efis</property>
            <value>15</value>
        </greater-than>
    </condition>
        <name>mask</name>
        <x>128</x>
        <y>128</y>
        <w>256</w>
        <h>256</h>
    </instrument>
    

    <instrument include="loc-scale.xml">
    <condition>
        <and>
            <greater-than>
               <property>systems/electrical/outputs/efis</property>
                <value>15</value>
            </greater-than>
            <property>instrumentation/nav/in-range</property>
        </and>
    </condition>
        <name>loc-scale</name>
        <x>113</x>
        <y>66</y>
        <w>78</w>
        <h>8</h>
    </instrument>

    <instrument include="gs-scale.xml">
    <condition>
        <and>
            <greater-than>
                <property>systems/electrical/outputs/efis</property>
                <value>15</value>
            </greater-than>
            <property>instrumentation/nav/has-gs</property>
        </and>
    </condition>
        <name>gs-scale</name>
        <x>178.0</x>
        <y>132.2</y>
        <w>8</w>
        <h>78</h>
    </instrument>   
</instruments>
  
</PropertyList>
