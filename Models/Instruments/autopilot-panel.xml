    <!--PICK ANIMATIONS-->
     <animation>
        <type>pick</type>
        <object-name>CMD-A</object-name>
        <visible>1</visible>
  <action>
    <name>Toggle CMDA</name>
   <button>0</button>
   <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        autopilot737.cmda_button_press();
      </script>   
    </binding>
  </action>
      </animation>

      <animation>
        <type>pick</type>
        <object-name>DISENGAGE</object-name>
        <visible>1</visible>
          <action>
  <name>AP disengage</name>
   <button>0</button>
   <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        autopilot737.apdsng_button_press();
      </script>   
    </binding>
  </action>
      </animation>

      <animation>
        <type>pick</type>
        <object-name>at-arm</object-name>
        <visible>1</visible>
  <action>
    <name>AT Arm Switch</name>
   <button>0</button>
   <repeatable>false</repeatable> 
   <binding>
    <command>property-toggle</command>
    <property>/autopilot/internal/SPD</property>
    <step>1</step>
    <min>0></min>
    <max>1</max>
    <wrap>true</wrap>
   </binding>
  </action>
      </animation>

