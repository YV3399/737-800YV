<?xml version="1.0"?>

<PropertyList>

 <path>autopilot-panel.ac</path>

    <animation>
        <type>rotate</type>
        <object-name>CPT-COURSE</object-name>
         <property>/instrumentation/nav[0]/radials/selected-deg</property> 
          <factor>-5.0</factor>         
        <center>
            <x-m>0.033</x-m>
            <y-m>-0.222</y-m>
            <z-m>0.0231</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation> 

