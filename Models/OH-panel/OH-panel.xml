<!-- Electrical Switches -->

	<animation>
		<type>select</type>
		<object-name>apugen</object-name>
		<condition>
			<or>
				<and>
					<equals>
						<property>controls/electrical/apu/Lsw</property>
						<value>1</value>
					</equals>
					<equals>
						<property>controls/electrical/apu/Rsw</property>
						<value>1</value>
					</equals>
					<greater-than>
						<property>systems/apu/rpm</property>
						<value>94.9</value>
					</greater-than>
				</and>
				<less-than-equals>
					<property>systems/apu/rpm</property>
					<value>94.9</value>
				</less-than-equals>
			</or>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>apugenO</object-name>
		<condition>
			<and>
				<or>
					<not-equals>
						<property>controls/electrical/apu/Lsw</property>
						<value>1</value>
					</not-equals>
					<not-equals>
						<property>controls/electrical/apu/Rsw</property>
						<value>1</value>
					</not-equals>
				</or>
				<greater-than>
					<property>systems/apu/rpm</property>
					<value>94.9</value>
				</greater-than>
			</and>
		</condition>
	</animation>
	
	<animation>
