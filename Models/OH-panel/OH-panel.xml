<!-- Battery Switch -->

<animation>
  <type>pick</type>
  <object-name>BatterySwitch</object-name>         
  <visible>1</visible>
  <action>
    <button>0</button>
    <repeatable type="bool">false</repeatable>     
    <binding>
      <command>property-toggle</command>
      <property>controls/electric/battery-switch</property>
      <command>nasal</command>
    </binding>
    <binding>
     <command>nasal</command>
     <script>
      if(getprop("/controls/electric/battery-switch") == 0) screen.log.write("Battery Master Switch OFF",1,0,0); 
      else 
      screen.log.write("Battery Master Switch ON",0,1,0);
    </script>
  </binding>
  <binding>
    <command>nasal</command>
    <script>props.globals.getNode("controls/switches/switch").setBoolValue(1);</script>
  </binding>
</action> 
</animation>
