		<rumble>
			<name>rumble</name>
			<mode>looped</mode>
			<path>Aircraft/737-800YV/Sounds/rumble.wav</path>
			<condition>
				<equals>
					<property>/fdm/jsbsim/gear/wow</property>
					<value>1</value>
				</equals>
				<greater-than>
					<property>/velocities/groundspeed-kt</property>
					<value>5</value>
				</greater-than>
			</condition>
			<volume>
				<property>/velocities/airspeed-kt</property>
				<type>log</type>
				<factor>0.6</factor>
			</volume>
		</rumble>
		
		<flap>
			<name>flaps</name>
			<mode>in-transit</mode>
			<path>Aircraft/737-800YV/Sounds/flaps.wav</path>
			<property>/surface-positions/flap-pos-norm</property>
			<volume>
				<property>sim/current-view/z-offset-m</property>
				<offset>1</offset>
				<factor>0.4</factor>
				<min>-1</min>
				<max>0.6</max>
			</volume>
		</flap>

		<gear>
			<name>gear</name>
			<mode>in-transit</mode>
			<path>Aircraft/737-800YV/Sounds/gear.wav</path>
			<property>/gear/gear[0]/position-norm</property>
			<pitch>
				<internal>dt_play</internal>
				<offset>1.0</offset>
				<factor>-0.001</factor>
			</pitch>
		</gear>
		
		<overspeed>
			<name>Overspeed warning</name>
			<path>Aircraft/737-800YV/Sounds/overspeed.wav</path>
			<mode>looped</mode>
			<condition>
				<greater-than>
					<property>velocities/airspeed-kt</property>
					<value>345</value>
				</greater-than>
			</condition>
			<volume>
				<factor>1.0</factor>
			</volume>
		</overspeed>
		
		<!-- Altitude alert sound -->

