		</ap-disconnect>

		<!-- trimwheel sound -->

		<trimming>
			<name>trimming</name>
			<mode>once</mode>
			<path>Aircraft/737-800YV/Sounds/trim.wav</path>
			<condition>
				<property>/b737/sound/stab-trim</property>
			</condition>
			<volume>
				<factor>1</factor>
			</volume>
		</trimming>
		
		<stall>
			<name>stall</name>
			<mode>loop</mode>
			<path>Aircraft/737-800YV/Sounds/stall.wav</path>
			<condition>
				<equals>
					<property>/b737/sound/stall</property>
					<value>1</value>
				</equals>
			</condition>
		</stall>
		
		<!-- Ground Services-->

		<fuelpump>
			<name>fuel truck pump transfer</name>
			<path>/Aircraft/737-800YV/Sounds/electricalengine.wav</path>
			<mode>looped</mode>
			<condition>
				<less-than>	
					<property>/velocities/groundspeed-kt</property>
					<value>1</value>
				</less-than>
			</condition>
			<volume>
				<property>/services/fuel-truck/transfer</property>
				<value>1</value>
			</volume>
        <!--volume>
            <property>sim/sound/Ovolume</property>
        </volume-->
        <position>
        	<x-m>0.25</x-m>
        	<y-m>-1.0</y-m>
        	<z-m>1.0</z-m>
        </position>
        <reference-dist>60.0</reference-dist>
        <max-dist>50.0</max-dist>
    </fuelpump>

