<?xml version="1.0"?>

<!-- B737-800 CWS System by Joshua Davidson (it0uchpods) -->
<!-- For Roll, Pitch, and Yaw -->

<PropertyList>

	<filter>
		<debug>false</debug>
		<type>noise-spike</type>
		<input>/controls/flight/aileron</input>
		<output>/controls/flight/aileron-feed</output>
		<max-rate-of-change>2.5</max-rate-of-change>
	</filter>
	
	<filter>
		<debug>false</debug>
		<type>noise-spike</type>
		<input>/controls/flight/elevator</input>
		<output>/controls/flight/elevator-feed</output>
		<max-rate-of-change>2.5</max-rate-of-change>
	</filter>

 <!-- =============================================================== -->
 <!-- Lateral                                                         -->
 <!-- =============================================================== -->
	
	<pid-controller>
		<name>IT-FBW: ROLL</name>
		<debug>false</debug>
		<enable>
			<condition>
				<and>
					<equals>
						<property>/it-autoflight/output/ap1</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/ap2</property>
						<value>0</value>
					</equals>
					<or>
						<equals>
							<property>/it-cws/cwsa-output</property>
							<value>1</value>
						</equals>
						<equals>
							<property>/it-cws/cwsb-output</property>
							<value>1</value>
						</equals>
					</or>
					<less-than>
						<property>/controls/flight/aileron-feed</property>
						<value>0.05</value>
					</less-than>
					<greater-than>
						<property>/controls/flight/aileron-feed</property>
						<value>-0.05</value>
					</greater-than>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>
			<property>/orientation/roll-rate-degps</property>
		</input>
		<reference>
			<value>0</value>
		</reference>
		<output>
			<property>/controls/flight/aileron-fbw-cmd</property>
		</output>
		<config>
			<Kp>
				<expression>
					<table>
						<property>/fdm/jsbsim/velocities/vc-kts</property>
						<entry><ind>140</ind><dep>0.14</dep></entry>
						<entry><ind>360</ind><dep>0.05</dep></entry>
					</table>
				</expression>
			</Kp>
			<beta>1</beta>
			<alpha>0.1</alpha>
			<gamma>0</gamma>
			<Ti>1.0</Ti>
			<Td>0.0001</Td>
			<u_min>-0.7</u_min>
			<u_max>0.7</u_max>
		</config>
	</pid-controller>
	
	<filter>
		<name>IT-FBW: ROLL OPTION</name>
		<debug>false</debug>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>
			<condition>
				<and>
					<equals>
						<property>/it-autoflight/output/ap1</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/ap2</property>
						<value>0</value>
					</equals>
					<or>
						<equals>
							<property>/it-cws/cwsa-output</property>
							<value>1</value>
						</equals>
						<equals>
							<property>/it-cws/cwsb-output</property>
							<value>1</value>
						</equals>
					</or>
					<less-than>
						<property>/controls/flight/aileron-feed</property>
						<value>0.05</value>
					</less-than>
					<greater-than>
						<property>/controls/flight/aileron-feed</property>
						<value>-0.05</value>
					</greater-than>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
			<property>/controls/flight/aileron-fbw-cmd</property>
		</input>
		<input>
			<condition>
				<or>
					<equals>
						<property>/it-autoflight/output/ap1</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/ap2</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/it-cws/cwsa-output</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/it-cws/cwsb-output</property>
						<value>0</value>
					</equals>
					<less-than-equals>
						<property>/controls/flight/aileron-feed</property>
						<value>-0.05</value>
					</less-than-equals>
					<greater-than-equals>
						<property>/controls/flight/aileron-feed</property>
						<value>0.05</value>
					</greater-than-equals>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>1</value>
					</equals>
				</or>
			</condition>
			<property>/controls/flight/aileron</property>
		</input>
		<output>/controls/flight/aileron-fde</output>
		<type>noise-spike</type>
		<max-rate-of-change>10</max-rate-of-change>
	</filter>
	
