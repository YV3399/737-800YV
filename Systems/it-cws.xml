						<value>0</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/ap2</property>
						<value>0</value> 
					</equals>
					<or>
						<equals>
							<property>/it-cws/cwsa-output</property>
							<value>1</value>
						</equals>
						<equals>
							<property>/it-cws/cwsb-output</property>
							<value>1</value>
						</equals>
					</or>
					<less-than>
						<property>/controls/flight/elevator-feed</property>
						<value>0.05</value>
					</less-than>
					<greater-than>
						<property>/controls/flight/elevator-feed</property>
						<value>-0.05</value>
					</greater-than>
					<equals>
						<property>/gear/gear[1]/wow</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/gear/gear[2]/wow</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>
			<property>/orientation/pitch-rate-degps</property>
		</input>
		<reference>
			<value>0</value>
		</reference>
		<output>
			<property>/controls/flight/elevator-fbw-cmd</property>
		</output>
		<config>
			<Kp>
				<expression>
					<table>
						<property>/fdm/jsbsim/velocities/vc-kts</property>
						<entry><ind>140</ind><dep>-0.28</dep></entry>
						<entry><ind>360</ind><dep>-0.09</dep></entry>
					</table>
				</expression>
			</Kp>
			<beta>1</beta>
			<alpha>0.1</alpha>
			<gamma>0</gamma>
			<Ti>0.45</Ti>
			<Td>0.001</Td>
			<u_min>-0.3</u_min>
			<u_max>0.3</u_max>
		</config>
	</pid-controller>
	
