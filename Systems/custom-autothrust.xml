<?xml version="1.0"?>

<!-- IT-AUTOFLIGHT: AUTOTHRUST -->

<PropertyList>

 <!-- =============================================================== -->
 <!-- Thrust Modes                                                    -->
 <!-- =============================================================== -->

	<!-- Auto throttle --> 
	<pid-controller>
		<name>IAS THR</name>
		<debug>false</debug>
		<enable>
			<condition>
				<and>
					<equals>
						<property>/it-autoflight/input/kts-mach</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/thr-mode</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/retard</property>
						<value>0</value>
					</equals>					
					<equals>
						<property>/it-autoflight/output/athr</property>
						<value>1</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>
			<property>/it-autoflight/internal/lookahead-10-sec-airspeed-kt</property>
		</input>
		<reference>
			<property>/it-autoflight/input/spd-kts</property>
		</reference>
		<output>
			<property>/controls/engines/target-thrust-cmd</property>
		</output>
		<config>
			<Kp>
				<property>/controls/engines/kp-spd</property>
			</Kp>    
			<beta>1.0</beta>    
			<alpha>0.1</alpha>  
			<gamma>0.0</gamma>  
			<Ti>01.0</Ti>       
			<Td>0.0001</Td>    
			<u_min>31</u_min>   
			<u_max>
				<property>/controls/engines/n1limx100</property>
			</u_max> 
		</config>
	</pid-controller>

	<!-- Auto throttle (Mach Hold)-->

	<pid-controller>
		<name>MACH THR</name>
		<debug>false</debug>
		<enable>
			<condition>
				<and>
					<equals>
						<property>/it-autoflight/input/kts-mach</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/thr-mode</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/retard</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/athr</property>
						<value>1</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>
			<property>/it-autoflight/internal/lookahead-10-sec-mach</property>
			<scale>1000.0</scale>
		</input>
		<reference>
			<property>/it-autoflight/input/spd-mach</property>
			<scale>1000.0</scale>
		</reference>
		<output>
			<property>/controls/engines/target-thrust-cmd</property>
		</output>
		<config>
			<Kp>
				<property>/controls/engines/kp-spd</property>
			</Kp>     
			<beta>1.0</beta>    
			<alpha>0.1</alpha>  
			<gamma>0.0</gamma>  
			<Ti>01.0</Ti>       
			<Td>0.0001</Td>    
			<u_min>31</u_min>   
			<u_max>
				<property>/controls/engines/n1limx100</property>
			</u_max> 
		</config>
	</pid-controller>

	<!-- IDLE EPR -->
	<pid-controller>
		<name>IDLE</name>
		<debug>false</debug>
		<enable>
			<condition>
				<and>
					<or>
						<equals>
							<property>/it-autoflight/output/thr-mode</property>
							<value>1</value>
						</equals>
						<equals>
							<property>/it-autoflight/output/retard</property>
							<value>1</value>
						</equals>
					</or>
					<equals>
						<property>/it-autoflight/output/athr</property>
						<value>1</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>
			<value>900</value>
		</input>
		<reference>
			<value>0</value>
		</reference>
		<output>
			<property>/controls/engines/target-thrust-cmd</property>
		</output>
		<config>
			<Kp>0.1</Kp>        
			<beta>1.0</beta>    
			<alpha>0.1</alpha>  
			<gamma>0.0</gamma>  
			<Ti>10.0</Ti>       
			<Td>0.00001</Td>    
			<u_min>31</u_min>   
			<u_max>
				<property>/controls/engines/n1limx100</property>
			</u_max> 
		</config>
	</pid-controller>

	<!-- CLB EPR -->
	<pid-controller>
		<name>CLB</name>
		<debug>false</debug>
		<enable>
			<condition>
				<and>
					<equals>
						<property>/it-autoflight/output/thr-mode</property>
						<value>2</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/retard</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/athr</property>
						<value>1</value>
					</equals>
				</and>
			</condition>
		</enable>
		<input>
			<value>0</value>
		</input>
		<reference>
			<value>900</value>
		</reference>
		<output>
			<property>/controls/engines/target-thrust-cmd</property>
		</output>
		<config>
			<Kp>0.1</Kp>        
			<beta>1.0</beta>    
			<alpha>0.1</alpha>  
			<gamma>0.0</gamma>  
			<Ti>10.0</Ti>       
			<Td>0.00001</Td>    
			<u_min>31</u_min>   
			<u_max>
				<property>/controls/engines/n1limx100</property>
			</u_max> 
		</config>
	</pid-controller>
	
	<filter>
		<name>TARGET THRUST CMD ONE</name>
		<debug>false</debug>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr</property>
					<value>0</value>
				</equals>
			</condition>
			<property>/engines/engine[0]/n1</property>
		</input>
		<input>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr</property>
					<value>1</value>
				</equals>
			</condition>
			<property>/controls/engines/target-thrust-cmd</property>
		</input>
		<output>
			<property>/controls/engines/target-thrust-one</property>
		</output>
		<type>noise-spike</type>
		<max-rate-of-change>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr</property>
					<value>0</value>
				</equals>
			</condition>
			<value>500</value>
		</max-rate-of-change>
		<max-rate-of-change>
			<condition>
				<and>
					<equals>
						<property>/it-autoflight/output/athr</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/thr-mode</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/retard</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
			<property>/controls/engines/n1-roc-thr</property>
		</max-rate-of-change>
		<max-rate-of-change>
			<condition>
				<and>
					<equals>
						<property>/it-autoflight/output/athr</property>
						<value>1</value>
					</equals>
					<not-equals>
						<property>/it-autoflight/output/thr-mode</property>
						<value>0</value>
					</not-equals>
					<equals>
						<property>/it-autoflight/output/retard</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
			<property>/controls/engines/n1-roc-pitch</property>
		</max-rate-of-change>
		<max-rate-of-change>
			<condition>
				<and>
					<equals>
						<property>/it-autoflight/output/athr</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/retard</property>
						<value>1</value>
					</equals>
				</and>
			</condition>
			<property>/controls/engines/n1-roc-retard</property>
		</max-rate-of-change>
	</filter>
	<filter>
		<name>TARGET THRUST CMD TWO</name>
		<debug>false</debug>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<input>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr</property>
					<value>0</value>
				</equals>
			</condition>
			<property>/engines/engine[1]/n1</property>
		</input>
		<input>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr</property>
					<value>1</value>
				</equals>
			</condition>
			<property>/controls/engines/target-thrust-cmd</property>
		</input>
		<output>
			<property>/controls/engines/target-thrust-two</property>
		</output>
		<type>noise-spike</type>
		<max-rate-of-change>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr</property>
					<value>0</value>
				</equals>
			</condition>
			<value>500</value>
		</max-rate-of-change>
		<max-rate-of-change>
			<condition>
				<and>
					<equals>
						<property>/it-autoflight/output/athr</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/thr-mode</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/retard</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
			<property>/controls/engines/n1-roc-thr</property>
		</max-rate-of-change>
		<max-rate-of-change>
			<condition>
				<and>
					<equals>
						<property>/it-autoflight/output/athr</property>
						<value>1</value>
					</equals>
					<not-equals>
						<property>/it-autoflight/output/thr-mode</property>
						<value>0</value>
					</not-equals>
					<equals>
						<property>/it-autoflight/output/retard</property>
						<value>0</value>
					</equals>
				</and>
			</condition>
			<property>/controls/engines/n1-roc-pitch</property>
		</max-rate-of-change>
		<max-rate-of-change>
			<condition>
				<and>
					<equals>
						<property>/it-autoflight/output/athr</property>
						<value>1</value>
					</equals>
					<equals>
						<property>/it-autoflight/output/retard</property>
						<value>1</value>
					</equals>
				</and>
			</condition>
			<property>/controls/engines/n1-roc-retard</property>
		</max-rate-of-change>
	</filter>

	<!-- EPR LIM -->
	<pid-controller>
		<name>N11</name>
		<debug>false</debug>
		<enable>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr</property>
					<value>1</value>
				</equals>
			</condition>
		</enable>
		<input>
			<property>/engines/engine[0]/n1</property>
		</input>
		<reference>
			<property>/controls/engines/target-thrust-one</property>
		</reference>
		<output>
			<property>/controls/engines/engine[0]/throttle-cmd</property>
		</output>
		<config>
			<Kp>
				<property>/controls/engines/kp</property>
			</Kp>
			<beta>1.0</beta>
			<alpha>0.1</alpha>
			<gamma>0.0</gamma>
			<Ti>2.0</Ti>
			<Td>0.0001</Td>
			<u_min>0.0</u_min>
			<u_max>1.0</u_max>
		</config>
	</pid-controller>

	<pid-controller>
		<name>N12</name>
		<debug>false</debug>
		<enable>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr</property>
					<value>1</value>
				</equals>
			</condition>
		</enable>
		<input>
			<property>/engines/engine[1]/n1</property>
		</input>
		<reference>
			<property>/controls/engines/target-thrust-two</property>
		</reference>
		<output>
			<property>/controls/engines/engine[1]/throttle-cmd</property>
		</output>
		<config>
			<Kp>
				<property>/controls/engines/kp</property>
			</Kp>
			<beta>1.0</beta>
			<alpha>0.1</alpha>
			<gamma>0.0</gamma>
			<Ti>2.0</Ti>
			<Td>0.0001</Td>
			<u_min>0.0</u_min>
			<u_max>1.0</u_max>
		</config>
	</pid-controller>
	
	<filter>
		<name>IT-CONTROLLER: N1 ENGINE 1</name>
		<debug>false</debug>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<enable>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr</property>
					<value>1</value>
				</equals>
			</condition>
		</enable>
		<input>
			<property>/controls/engines/engine[0]/throttle-cmd</property>
		</input>
		<output>
			<property>/controls/engines/engine[0]/throttle</property>
		</output>
		<type>noise-spike</type>
		<max-rate-of-change>
			<value>0.40</value>
		</max-rate-of-change>
	</filter>
	
	<filter>
		<name>IT-CONTROLLER: N1 ENGINE 2</name>
		<debug>false</debug>
		<feedback-if-disabled>true</feedback-if-disabled>
		<initialize-to>output</initialize-to>
		<enable>
			<condition>
				<equals>
					<property>/it-autoflight/output/athr</property>
					<value>1</value>
				</equals>
			</condition>
		</enable>
		<input>
			<property>/controls/engines/engine[1]/throttle-cmd</property>
		</input>
		<output>
			<property>/controls/engines/engine[1]/throttle</property>
		</output>
		<type>noise-spike</type>
		<max-rate-of-change>
			<value>0.40</value>
		</max-rate-of-change>
	</filter>

</PropertyList>
