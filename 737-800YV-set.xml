<?xml version="1.0"?>

<PropertyList include="737ng-common.xml">

	<sim>
		<description>Boeing 737-800YV</description>
		<liveries-path type="string">Aircraft/737-800YV/Models/Liveries-800</liveries-path>

		<aero>737-800YV</aero>
		<startup>
                        <splash-texture>Aircraft/737-800YV/Splash/splash-1.png</splash-texture>
                        <splash-texture>Aircraft/737-800YV/Splash/splash-2.png</splash-texture>
                        <splash-texture>Aircraft/737-800YV/Splash/splash-3.png</splash-texture>
                        <splash-texture>Aircraft/737-800YV/Splash/splash-4.png</splash-texture>
                        <splash-texture>Aircraft/737-800YV/Splash/splash-5.png</splash-texture>
                        <splash-texture>Aircraft/737-800YV/Splash/splash-6.png</splash-texture>
               </startup>

		<previews>
			<preview>
				<type>exterior</type>
				<splash type="bool">true</splash>
				<path>Splash/splash-1.png</path>
			</preview>
		</previews>

		 <model>
			<path>Aircraft/737-800YV/Models/737-800.xml</path>       
			<livery>
				<file>BOE</file>
				<newwinglets type="bool">false</newwinglets>   
			</livery>

			<rendering>
				<rain-enabled type="int">2</rain-enabled>
				<reflection-enabled type="int">1</reflection-enabled>
				<reflection-type type="int">1</reflection-type>
				<reflection-dynamic type="int">1</reflection-dynamic>
				<reflection-correction type="float">-0.8</reflection-correction>
				<reflection-fresnel type="float">0.0</reflection-fresnel>
				<reflection-fresnel-factor type="float">0.7</reflection-fresnel-factor>
				<reflection-rainbow type="float">0.0</reflection-rainbow>
				<reflection-noise type="float">0.00</reflection-noise>
				<ambient-correction type="float">0.15</ambient-correction>
			</rendering>
		</model> 
   
		<rating>
			<FDM type="int">3</FDM>
			<systems type="int">3</systems>
			<cockpit type="int">3</cockpit>
			<model type="int">4</model>
		</rating>

		<view>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<from-model-idx type="int">0</from-model-idx>
				<default-field-of-view-deg type="double">65</default-field-of-view-deg>
				<pitch-offset-deg>-16</pitch-offset-deg>
				<heading-offset-deg>0</heading-offset-deg>
				<x-offset-m archive="y">-0.51</x-offset-m>
				<y-offset-m archive="y">1.28</y-offset-m>
				<z-offset-m archive="y">-16.98</z-offset-m>
			</config>
		</view>

		<view n="106">
			<name>Copilot</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<from-model-idx type="int">0</from-model-idx>
				<default-field-of-view-deg type="double">65</default-field-of-view-deg>
				<pitch-offset-deg>-16</pitch-offset-deg>
				<heading-offset-deg>0</heading-offset-deg>
				<x-offset-m archive="y">0.51</x-offset-m>
				<y-offset-m archive="y">1.28</y-offset-m>
				<z-offset-m archive="y">-16.98</z-offset-m>
			</config>   
		</view>

		<view n="100">
			<name>CDU View</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<from-model-idx type="int">0</from-model-idx>
				<default-field-of-view-deg type="double">41</default-field-of-view-deg>
				<pitch-offset-deg>-70</pitch-offset-deg>
				<heading-offset-deg>0</heading-offset-deg>
				<x-offset-m archive="y">-0.15</x-offset-m>
				<y-offset-m archive="y">1.3</y-offset-m>
				<z-offset-m archive="y">-17.6</z-offset-m>
			</config>
		</view>	
		<view n="101">
			<name>Engine Left</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<from-model-idx type="int">0</from-model-idx>
				<default-field-of-view-deg type="double">62</default-field-of-view-deg>
				<pitch-offset-deg>0</pitch-offset-deg>
				<heading-offset-deg>130</heading-offset-deg>
				<x-offset-m archive="y">-1.83</x-offset-m>
				<y-offset-m archive="y">1.2</y-offset-m>
				<z-offset-m archive="y">-9.02</z-offset-m>
			</config>   
		</view>
		<view n="102">
			<name>Engine Right</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<from-model-idx type="int">0</from-model-idx>
				<default-field-of-view-deg type="double">62</default-field-of-view-deg>
				<pitch-offset-deg>0</pitch-offset-deg>
				<heading-offset-deg>-130</heading-offset-deg>
				<x-offset-m archive="y">1.83</x-offset-m>
				<y-offset-m archive="y">1.2</y-offset-m>
				<z-offset-m archive="y">-9.02</z-offset-m>
			</config>   
		</view>
		
                <view n="103">
			<name>Right Wing</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<from-model-idx type="int">0</from-model-idx>
				<default-field-of-view-deg type="double">80</default-field-of-view-deg>
				<pitch-offset-deg>0</pitch-offset-deg>
				<heading-offset-deg>-90</heading-offset-deg>
				<x-offset-m archive="y">1.80</x-offset-m>
				<y-offset-m archive="y">0.91</y-offset-m>
				<z-offset-m archive="y">2.19</z-offset-m>
			</config>
		</view>
		
                <view n="104">
			<name>Left Wing</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<from-model-idx type="int">0</from-model-idx>
				<default-field-of-view-deg type="double">80</default-field-of-view-deg>
				<pitch-offset-deg>0</pitch-offset-deg>
				<heading-offset-deg>90</heading-offset-deg>
				<x-offset-m archive="y">-1.80</x-offset-m>
				<y-offset-m archive="y">0.91</y-offset-m>
				<z-offset-m archive="y">2.19</z-offset-m>
			</config>
		</view>
		
                <view n="105">
			<name>Overhead Panel</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<from-model-idx type="int">0</from-model-idx>
				<default-field-of-view-deg type="double">108</default-field-of-view-deg>
				<pitch-offset-deg>50</pitch-offset-deg>
				<heading-offset-deg>0</heading-offset-deg>
				<x-offset-m archive="y">0</x-offset-m>
				<y-offset-m archive="y">1.26</y-offset-m>
				<z-offset-m archive="y">-17.26</z-offset-m>
			</config>
		</view>
	</sim>

	<limits> <!-- based on EI-RUA aircraft data -->
		<mass-and-balance>
			<maximum-ramp-mass-lbs>173000</maximum-ramp-mass-lbs>
			<maximum-takeoff-mass-lbs>172500</maximum-takeoff-mass-lbs>
			<maximum-landing-mass-lbs>144076</maximum-landing-mass-lbs>
			<maximum-zero-fuel-mass-lbs>136000</maximum-zero-fuel-mass-lbs>
		</mass-and-balance>
		<max-flap-extension-speed>
			<flaps>0.125</flaps>
			<speed>250</speed>
		</max-flap-extension-speed>
		<max-flap-extension-speed>
			<flaps>0.250</flaps>
			<speed>250</speed>
		</max-flap-extension-speed>
		<max-flap-extension-speed>
			<flaps>0.375</flaps>
