		</max-flap-extension-speed>
		<max-flap-extension-speed>
			<flaps>0.625</flaps>
			<speed>200</speed>
		</max-flap-extension-speed>
		<max-flap-extension-speed>
			<flaps>0.750</flaps>
			<speed>190</speed>
		</max-flap-extension-speed>
		<max-flap-extension-speed>
			<flaps>0.875</flaps>
			<speed>175</speed>
		</max-flap-extension-speed>
		<max-flap-extension-speed>
			<flaps>1.000</flaps>
			<speed>158</speed>
		</max-flap-extension-speed>
		<max-gear-extension-speed>270</max-gear-extension-speed>
		<max-gear-retraction-speed>235</max-gear-retraction-speed>
		<vne>340</vne>
		<max-positive-g>2.5</max-positive-g> 
	</limits>

	<payload>
		<pax>
			<business type="double">6</business>
			<tourist type="double">22</tourist>
			<economy type="double">50</economy>
		</pax>
		<weight>
